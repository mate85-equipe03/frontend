    "stylelint": "stylelint --max-warnings 0 --color src/**/*.css",
    "stylelint-fix": "stylelint src/**/*.css --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint-fix": "npm run eslint-fix && npm run stylelint-fix"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.16.0",
