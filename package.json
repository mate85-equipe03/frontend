{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prod": "npm run build && npm run preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
