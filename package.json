    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@mui/x-data-grid": "^5.17.6",
    "@mui/x-date-pickers": "^5.0.9",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "dayjs": "^1.11.6",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.0.1",
    "react-router-dom": "^6.3.0"
